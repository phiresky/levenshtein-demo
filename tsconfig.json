{
	"compilerOptions": {
		"target": "es5",
		"outDir": "bin",
		"sourceMap": true,
		"module": "commonjs",
		"jsx": "react",
		"strict": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true
	},
	"exclude": ["node_modules"]
}
